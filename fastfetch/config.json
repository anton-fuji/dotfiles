{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",

  "logo": {
    "type": "file",
    "source": "~/.config/fastfetch/ascii.txt",
    "color": {
      "1": "#B4BEFE",
      "2": "#A6ADC8",
      "3": "#89B4FA",
      "4": "#74C7EC",
      "5": "#89DCEB",
      "6": "#94E2D5",
      "7": "#7DCFFF",
      "8": "#5CCFE6",
      "9": "#4AA5F0"
    },
    "padding": {
      "top": 1,
      "right": 3
    }
  },

  "display": {
    "separator": " "
  },

  "modules": [
    "break",

    {
      "type": "title",
      "color": {
        "user": "#89B4FA",
        "at": "#A6ADC8",
        "host": "#74C7EC"
      }
    },

    "break",

    {
      "type": "os",
      "key": "",
      "keyColor": "#74C7EC"
    },
    {
      "type": "uptime",
      "key": "󰔟",
      "keyColor": "#89DCEB"
    },
    {
      "type": "datetime",
      "key": "󰃰",
      "keyColor": "#5CCFE6",
      "format": "{year}-{month}-{day} {hour}:{minute}"
    },

    "break",

    {
      "type": "cpu",
      "key": "",
      "keyColor": "#89B4FA"
    },
    {
      "type": "gpu",
      "key": "󰍛",
      "keyColor": "#7DCFFF"
    },
    {
      "type": "memory",
      "key": "",
      "keyColor": "#94E2D5",
      "format": "{used} / {total} ({percentage})"
    },

    "break",

    {
      "type": "disk",
      "key": "",
      "keyColor": "#74C7EC"
    },
    {
      "type": "network",
      "key": "󰤨",
      "keyColor": "#5CCFE6"
    },

    "break",

    {
      "type": "colors",
      "symbol": "circle"
    }
  ]
}
